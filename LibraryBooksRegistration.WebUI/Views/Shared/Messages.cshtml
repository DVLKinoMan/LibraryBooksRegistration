@if (TempData["SuccessMessage"] != null)
            {
    <div class="alert alert-success">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">
            ×
        </button>
        <span class="glyphicon glyphicon-ok"></span> <strong>წარმატების მესიჯი</strong>
        <hr class="message-inner-separator">
        <p>
            @TempData["SuccessMessage"]
        </p>
    </div>
    @*<h5 class="alert alert-success"> @TempData["SuccessMessage"]</h5>*@
}
@if (TempData["ErrorMessage"] != null)
            {
    <div class="alert alert-danger">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">
            ×
        </button>
        <span class="glyphicon glyphicon-hand-right"></span> <strong>წარუმატებლობის მესიჯი</strong>
        <hr class="message-inner-separator">
        <p>
            @TempData["ErrorMessage"]
        </p>
    </div>
    @*<h5 class="alert alert-danger"> @TempData["ErrorMessage"]</h5>*@
}
@if (TempData["InfoMessage"] != null)
            {
    <div class="alert alert-info">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">
            ×
        </button>
        <span class="glyphicon glyphicon-info-sign"></span> <strong>ინფორმაციის მესიჯი</strong>
        <hr class="message-inner-separator">
        <p>
            @TempData["InfoMessage"]
        </p>
    </div>
}
